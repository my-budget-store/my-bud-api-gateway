{
  "Routes": [
    {
      "DownstreamPathTemplate": "/v1/products",
      "DownstreamScheme": "https",
      "SwaggerKey": "products",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7182
        }
      ],
      "UpstreamPathTemplate": "/mybud/products",
      "UpstreamHttpMethod": [ "Get" ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "apiGateway",
      "TransformByOcelotConfig": false,
      "Config": [
        {
          "Name": "MyBud.ApiGateway",
          "Version": "1.0",
          "Url": "http://localhost:5101/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "products",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Products API",
          "Version": "1.0",
          "Url": "https://localhost:7182/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}